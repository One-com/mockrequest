    "test",
    "unexpected"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/One-com/node-mockrequest/issues"
  },
  "homepage": "https://github.com/One-com/node-mockrequest",
  "dependencies": {
    "bufferedstream": "2.6.0",
    "httperrors": "0.5.0",
    "passerror": "1.0.1",
    "unexpected": "4.1.3"
  },
  "devDependencies": {
    "coveralls": "2.11.1",
    "istanbul": "0.3.0",
    "jshint": "2.5.4",
    "mocha": "1.21.4"
